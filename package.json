{
  "name": "vision",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "serve": "cross-env NODE_ENV=development vue-cli-service serve",
    "build": "cross-env BUILD_ENV=prod vue-cli-service build"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "compression-webpack-plugin": "^5.0.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "echarts": "^5.2.0",
    "less-loader": "^6.0.0",
    "pinyin": "^2.10.2",
    "vue": "^3.0.0",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "rules": {
      "global-require": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
